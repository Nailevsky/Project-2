<header>
    <nav>
      <ul>
        <% if(currentUser){ %>
        <li>Welcome <%= currentUser.username %></li>
        <li>
          <form action="/sessions/?_method=DELETE" method="POST">
            <input type="submit" value="Logout" class="logout-button"/>
          </form>
        </li>
  
        <% } else { %>
        <li><a href="/users/new">Sign Up</a></li>
        <li><a href="/session/new">Sign In</a></li>
  
        <% } %>
      </ul>
</header>